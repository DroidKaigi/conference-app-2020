<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="filterChipStyle" format="reference"/>

    <declare-styleable name="FilterChip">
        <attr name="android:checked"/>
        <attr name="android:text"/>
        <attr name="android:textColor"/>
        <attr name="android:textSize"/>
        <attr name="android:color"/>
        <attr name="android:fontFamily"/>
        <attr name="strokeColor"/>
        <attr name="strokeWidth"/>
        <attr name="backgroundColor" format="color"/>
        <attr name="horizontalPadding" format="dimension"/>
        <attr name="verticalPadding" format="dimension"/>
        <attr name="selectedTextColor" format="color"/>
        <attr name="clearIcon" format="reference"/>
        <attr name="clearIconTint" format="color"/>
        <attr name="foreground" format="reference"/>
        <attr name="showIcons" format="boolean"/>
        <attr name="dotSize" format="dimension"/>
        <attr name="dotPadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SearchHeader">
        <attr name="android:fontFamily" />
        <attr name="fontFamily" />
        <attr name="android:textColor" />
        <attr name="android:textSize" />
    </declare-styleable>

    <declare-styleable name="ChromeCustomTabsNavigator">
        <attr name="webUrl" format="boolean" />
    </declare-styleable>

    <!-- workaround for https://issuetracker.google.com/issues/136103084#comment23 -->
    <attr name="flow_horizontalSeparator"/>
    <attr name="flow_verticalSeparator"/>
    <attr name="motionProgress"/>
    <attr name="waveDecay"/>
    <attr name="motionPathRotate"/>
    <attr name="duration"/>
</resources>
